{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double", { "allowTemplateLiterals": true }],
        "import/prefer-default-export":"off",
        "linebreak-style":"off",
        "import/extensions":"off",
        "no-plusplus":"off",
        "no-underscore-dangle":"off"
    }
}